	.overlay {
		height: 100%;
		width: 100%;
		display: none;
		position: fixed;
		z-index: 2000;
		top: 0;
		left: 0;
		background-color: rgb(0,0,0);
		background-color: rgba(0,0,0, 0.9);
	}



		.overlay a {
			padding: 8px;
			text-decoration: none;
			font-size: 36px;
			color: #818181;
			display: block;
			transition: 0.3s;
		}

			.overlay a:hover, .overlay a:focus {
				color: #f1f1f1;
			}

		.overlay .closebtn {
			position: absolute;
			top: 20px;
			right: 45px;
			font-size: 60px;
		}

	.tableSchedule {
		text-align: center;
		background-color: white;
		width: 50%;
		position: relative;
		top: 50%;
		left: 25%;
	}

	.borderSe {
		border: 2px solid white !important;
	}

	.borderSethead {
		border: 2px solid white !important;
		background-color: #EC5B24;
		color: white;
