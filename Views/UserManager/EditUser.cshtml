							<h4 class="mb-4 h5 mt-5">User Information</h4>
							<form asp-action="EditUser">
								<div class="row">
									<!-- input -->
									<div class="mb-3 col-lg-6">
										<label class="form-label">Name</label>
										<input asp-for="UserName" class="form-control" placeholder="Name" required />
									</div>
									<!-- input -->
									<div class="mb-3 col-lg-6">
										<label class="form-label">Password</label>
										<input asp-for="Password" class="form-control" placeholder="password" required />
									</div>
									<div class="mb-3 col-lg-6">
										<label class="form-label">Email</label>
										<input asp-for="Email" class="form-control" placeholder="Email" required />
									</div>
									<div class="mb-3 col-lg-6">
										<label class="form-label">Phone</label>
										<input asp-for="Phone" class="form-control" placeholder="Phone" required />
									</div>

									<div></div>
									<!-- input -->
									<!-- input -->

									<div class="mb-3 col-lg-12 mt-5">
										<!-- input -->
										<div class="mb-3 col-lg-12">
											<label class="form-label">Role</label>
											<select class="form-select" asp-for="Role" name="Role">
												<option value="Passenger">Passenger</option>
												<option value="Admin">Admin</option>
											</select>
										</div>

									</div>
									<div class="col-lg-12">
										<input type="submit" class="btn btn-me" value="Create User" />
									</div>
